<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  <hibernate-mapping>
 <class name="com.nt.entity.BankAccount" table="OTM_UNI_BANKACCOUNT_MAP">
 	<id name="accountNo" type="long">
 	<generator class="sequence"/>
 	</id>
 	<property name="accountType" type="string" length="30"/>
 	<property name="bankName" type="string" length="30" />
 	<property name="balance" type="double"></property>
 </class>
  
  </hibernate-mapping>
  
 